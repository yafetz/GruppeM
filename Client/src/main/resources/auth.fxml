<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Controller.Auth.AuthenticationController">
   <children>
      <TextField fx:id="authfield" layoutX="212.0" layoutY="105.0" />
      <Text layoutX="128.0" layoutY="122.0" strokeType="OUTSIDE" strokeWidth="0.0" text="AuthCode" />
      <Button fx:id="submitbutton" layoutX="113.0" layoutY="200.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Button fx:id="cancelbutton" layoutX="361.0" layoutY="200.0" mnemonicParsing="false" onAction="#cancelPressedButton" text="Cancel" />
   </children>
</AnchorPane>
